{
  "name": "codemare-backend",
  "version": "0.1.0",
  "description": "Backend for Codemare online code editor",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "deploy": "./deploy.sh",
    "test:deployment": "./tests/deployment.test.sh",
    "docker:prod": "docker-compose -f docker-compose.production.yml up -d",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f backend",
    "docker:down": "docker-compose -f docker-compose.production.yml down"
  },
  "keywords": [
    "codemare",
    "backend",
    "code-editor"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.29",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2"
  }
}
